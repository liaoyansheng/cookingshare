/* pages/foodhome/foodhome.wxss */
.searchView{
  position: fixed;
  top:0rpx;
  background-color: #F3090D;
  width:750rpx;
  height:108rpx;
  padding:1rpx;
  z-index: 999;
}
.searchView_copy{
  width:750rpx;
  height:108rpx;
}
.search{
  display: flex;
  flex-direction: row;
  align-items:center;
  width: 642rpx;
  height:60rpx;
  /* background: #FFFF; */
  background-color: #FFFFFF;
  border-radius:30rpx;
  margin:24rpx;
  padding:0rpx 30rpx; 
}
.searchinput{
  width: 550rpx;
  height:50rpx;
  font-size:30rpx;
  margin-left:20rpx;
}
.swiperClass {
  margin-top: 10px;
}
 
.slide-image {
  width: 100%; 
  height: 90%;
  border-radius: 10px;
  position: relative;
  background-size: 100% 100%;
  background-repeat: no-repeat;
}
 
image.active {
  transform: none;
  transition: all 0.2s ease-in 0s;
} 
 
image.quiet {
  transform: scale(0.8333333);
  transition: all 0.2s ease-in 0s;
}
.foodpage{
  width: 750rpx;
  display: flex;
  flex-flow: row wrap;
}
.foodpage1{
  width: 750rpx;
  display: flex;
  flex-flow: row wrap;
}
.content{
  width: 345rpx;
  box-shadow: 0 6rpx 8rpx #eeeeee;
  border-radius: 10rpx;
  margin: 15rpx;
}
.content-img{
  width:100%;
  border-radius: 10rpx;
  background-repeat:no-repeat; 
  background-size: cover; 
}
.content-img1{
  width:100%;
  height: 300rpx;
  border-radius: 10rpx;
  background-repeat:no-repeat; 
  background-size: cover; 
}
.content-title{
  width: 345rpx;
  font-size: 30rpx;
  font-weight: 600;
  line-height: 40rpx;
  padding-left: 10rpx;
}
.content-more{
  width: 345rpx;
  font-size: 24rpx;
  line-height: 50rpx;
  color: #A2A1A1;
  padding:0 0 6px 10rpx;
}
.message{
  width: 710rpx;
  padding: 20rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  border-bottom: 8rpx solid #FFF1F1F1;
}
.message-title{
  width: 480rpx;
  font-size: 36rpx;
  max-height:148rpx;
  display: -webkit-box;
  word-break: break-all;
  text-overflow: ellipsis;
  overflow: hidden;
  -webkit-box-orient: vertical;
  -webkit-line-clamp:3;
}
.message-visit{
  width: 480rpx;
  font-size:24rpx;
  color:rgba(102,102,102,1);
  line-height:35rpx;  
  text-align:right;
}
.message-img{
  width:190rpx;
  height: 190rpx; 
}
.todaygood{
  width:730rpx;
  text-align: left;
  line-height: 60rpx;
  font-size: 40rpx;
  font-weight: 600;
  color: #FF6347;
  margin:0 20rpx;
  border-bottom: 8rpx solid #FFF1F1F1;
}
.thinklikely{
  width:730rpx;
